        <term_definitions code="at0000">
            <items id="text">Person</items>
            <items id="description">An individual human being.

LEON: This serves to store the "Aanvrager" concept.</items>
        </term_definitions>
        <term_definitions code="at0001">
            <items id="text">Name</items>
            <items id="description">The unstructured name for the individual.</items>
            <items id="comment">The content of this data element may be derived from one or more components from CLUSTER.structured_name combined together as a text string. For example: 'John Markham', 'Professor Sir John Markham', 'John Markham Jnr MP'.</items>
        </term_definitions>
        <term_definitions code="at0002">
            <items id="text">Structured name</items>
            <items id="description">Alternative representation of an individual's complete name by separation into discrete, structured components.</items>
            <items id="comment">Any or all of the structured name elements can be combined together as a text string and represented in the 'Name' data element in this archetype.</items>
        </term_definitions>
        <term_definitions code="at0003">
            <items id="text">Identifier</items>
            <items id="description">Identifier associated with the individual.</items>
            <items id="comment">Occurrences for this data element is set to 0..* to allow for more than one Identifier to be recorded. Note that the DV_IDENTIFIER data type contains multiple subcomponents for recording the ID value, type, issuer and assigned. For example - social security number; driver's license; or passport number.</items>
        </term_definitions>
        <term_definitions code="at0004">
            <items id="text">Role</items>
            <items id="description">The relationship or role of the individual to the subject of the health record.</items>
            <items id="comment">For example - the copyholder of an advance care record; contact person in an organisation; relative in a family history entry; specimen collector; or witness to a fall or accident. If the individual being described using this archetype is the subject of the health record, then this data element is redundant.</items>
        </term_definitions>
        <term_definitions code="at0005">
            <items id="text">Address</items>
            <items id="description">Details about an address for the individual.</items>
        </term_definitions>
        <term_definitions code="at0008">
            <items id="text">Additional details</items>
            <items id="description">Additional details about the individual.</items>
        </term_definitions>
        <term_definitions code="at0010">
            <items id="text">Comment</items>
            <items id="description">Additional narrative about the individual not captured in other fields.</items>
        </term_definitions>
        <term_definitions code="at0007">
            <items id="text">Organisation</items>
            <items id="description">Details about the organisational context for the individual.</items>
            <items id="comment">For example: identifying the business or community organisation associated with the 'Role' of the individual.</items>
        </term_definitions>
        <term_definitions code="at0006">
            <items id="text">Electronic communication</items>
            <items id="description">Details about one or more types of electronic communication for the individual.</items>
        </term_definitions>
        <term_definitions code="at0009">
            <items id="text">Photo</items>
            <items id="description">Photograph of the individual.</items>
        </term_definitions>
        <term_definitions code="at0011">
            <items id="text">Label</items>
            <items id="description">A label for the individual.</items>
            <items id="comment">For example: 'Neighbour in the house with the red door'.</items>
        </term_definitions>
